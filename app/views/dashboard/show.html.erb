<ol class="breadcrumb">
  <li><a><%= link_to "Home", root_path %></a></li>
  <li class="active">Dashboard</li>
</ol>
<br>
<h4>User Information </h4>
<% if current_user %>
  <h4> Name: <%= current_user.first_name %> <%= current_user.last_name %></h4>
  <h4> Email: <%= current_user.email %></h4>
  <h4> Address: <%= current_user.address %></h4>

  <%= link_to "Edit User Info", edit_admin_user_path(@current_user), class: "btn btn-primary" %>
<% end %>
